namespace WinFormsApp1
{
    partial class MainPageForm
    {
        /// <summary>
        ///  Required designer variable.
        /// </summary>
        private System.ComponentModel.IContainer components = null;

        /// <summary>
        ///  Clean up any resources being used.
        /// </summary>
        /// <param name="disposing">true if managed resources should be disposed; otherwise, false.</param>
        protected override void Dispose(bool disposing)
        {
            if (disposing && (components != null))
            {
                components.Dispose();
            }
            base.Dispose(disposing);
        }

        #region Windows Form Designer generated code

        /// <summary>
        ///  Required method for Designer support - do not modify
        ///  the contents of this method with the code editor.
        /// </summary>
        private void InitializeComponent()
        {
            mnuBar = new MenuStrip();
            calculatorToolStripMenuItem = new ToolStripMenuItem();
            mnuSumCalc = new ToolStripMenuItem();
            mnudCalc1 = new ToolStripSeparator();
            mnuSquareCalc = new ToolStripMenuItem();
            mnuSalary = new ToolStripMenuItem();
            mnuContact = new ToolStripMenuItem();
            mnuContactList = new ToolStripMenuItem();
            mnuNewContact = new ToolStripMenuItem();
            mnuBar.SuspendLayout();
            SuspendLayout();
            // 
            // mnuBar
            // 
            mnuBar.ImageScalingSize = new Size(20, 20);
            mnuBar.Items.AddRange(new ToolStripItem[] { calculatorToolStripMenuItem, mnuSalary, mnuContact });
            mnuBar.Location = new Point(0, 0);
            mnuBar.Name = "mnuBar";
            mnuBar.Size = new Size(1070, 28);
            mnuBar.TabIndex = 1;
            // 
            // calculatorToolStripMenuItem
            // 
            calculatorToolStripMenuItem.DropDownItems.AddRange(new ToolStripItem[] { mnuSumCalc, mnudCalc1, mnuSquareCalc });
            calculatorToolStripMenuItem.Name = "calculatorToolStripMenuItem";
            calculatorToolStripMenuItem.Size = new Size(90, 24);
            calculatorToolStripMenuItem.Text = "&Calculator";
            // 
            // mnuSumCalc
            // 
            mnuSumCalc.Name = "mnuSumCalc";
            mnuSumCalc.Size = new Size(209, 26);
            mnuSumCalc.Text = "&Sum Calculator";
            mnuSumCalc.Click += mnuSumCalc_Click;
            // 
            // mnudCalc1
            // 
            mnudCalc1.Name = "mnudCalc1";
            mnudCalc1.Size = new Size(206, 6);
            // 
            // mnuSquareCalc
            // 
            mnuSquareCalc.Name = "mnuSquareCalc";
            mnuSquareCalc.Size = new Size(209, 26);
            mnuSquareCalc.Text = "S&quare Calculator";
            mnuSquareCalc.Click += mnuSquareCalc_Click;
            // 
            // mnuSalary
            // 
            mnuSalary.Name = "mnuSalary";
            mnuSalary.Size = new Size(63, 24);
            mnuSalary.Text = "&Salary";
            mnuSalary.Click += mnuSalary_Click;
            // 
            // mnuContact
            // 
            mnuContact.DropDownItems.AddRange(new ToolStripItem[] { mnuContactList, mnuNewContact });
            mnuContact.Name = "mnuContact";
            mnuContact.Size = new Size(74, 24);
            mnuContact.Text = "C&ontact";
            // 
            // mnuContactList
            // 
            mnuContactList.Name = "mnuContactList";
            mnuContactList.Size = new Size(224, 26);
            mnuContactList.Text = "&Contact List";
            mnuContactList.Click += mnuContactList_Click;
            // 
            // mnuNewContact
            // 
            mnuNewContact.Name = "mnuNewContact";
            mnuNewContact.Size = new Size(224, 26);
            mnuNewContact.Text = "&New Contact";
            mnuNewContact.Click += mnuNewContact_Click;
            // 
            // MainPageForm
            // 
            AutoScaleDimensions = new SizeF(8F, 20F);
            AutoScaleMode = AutoScaleMode.Font;
            ClientSize = new Size(1070, 647);
            Controls.Add(mnuBar);
            IsMdiContainer = true;
            MainMenuStrip = mnuBar;
            Name = "MainPageForm";
            Text = "Example 1 App";
            WindowState = FormWindowState.Maximized;
            mnuBar.ResumeLayout(false);
            mnuBar.PerformLayout();
            ResumeLayout(false);
            PerformLayout();
        }

        #endregion

        private MenuStrip mnuBar;
        private ToolStripMenuItem calculatorToolStripMenuItem;
        private ToolStripMenuItem mnuSumCalc;
        private ToolStripSeparator mnudCalc1;
        private ToolStripMenuItem mnuSquareCalc;
        private ToolStripMenuItem mnuSalary;
        private ToolStripMenuItem mnuContact;
        private ToolStripMenuItem mnuContactList;
        private ToolStripMenuItem mnuNewContact;
    }
}

